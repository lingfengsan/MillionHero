import os
import sys

_SYSTRACE_DIR = os.path.abspath(
    os.path.join(os.path.dirname(__file__), os.path.pardir))
sys.path.insert(0, _SYSTRACE_DIR)
from systrace import run_systrace

if __name__ == '__main__':
  sys.exit(run_systrace.main())
